<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd"
                   logicalFilePath="path-independent">

    
    <!-- tables -->
    <include relativeToChangelogFile="true" file="./tables/create_table.changelog.xml"  context="snapcrescent"></include>

    <!-- functions -->
    <include relativeToChangelogFile="true" file="./functions/create_function.changelog.xml" context="snapcrescent"></include>
    
    <!-- views -->
    <include relativeToChangelogFile="true" file="./views/create_view.changelog.xml" context="snapcrescent"></include>

    <!-- trigger -->
    <include relativeToChangelogFile="true" file="./triggers/create_trigger.changelog.xml" context="snapcrescent"></include>

    <!-- SP -->
    <include relativeToChangelogFile="true" file="./storedProcedures/create_sp.changelog.xml" context="snapcrescent"></include>
    
    <!-- seeded data -->
    <include relativeToChangelogFile="true" file="./seededData/Insert_Seeded_Data.sql" context="snapcrescent-seed"></include>
        
    
</databaseChangeLog>
